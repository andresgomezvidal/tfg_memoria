<rule id="333100" level="0">
	<decoded_as>json</decoded_as>
	<field name="Total_of_free_bytes">\.+</field>
	<field name="Total_of_bytes">\.+</field>
	<field name="Total_of_avail_free_bytes">\.+</field>
	<description>Free space monitor of a volume</description>
</rule>
<rule id="333101" level="3" >
	<if_sid>333100</if_sid>
	<field name="Total_of_free_bytes">\.+</field>
	<description>New event from the free space monitor of a volume</description>
</rule>

<rule id="333102" level="0">
	<if_group>syscheck</if_group>
	<match>/var/log/ossec-custom/</match>
	<description>Syscheck integrity check on custom output files</description>
</rule>
<rule id="333103" level="3">
	<if_sid>333102</if_sid>
	<match>volume</match>
	<description>Syscheck integrity check of the free space of a volume</description>
</rule>

<rule id="333110" level="3" timeframe="50" frequency="2" >
	<if_matched_sid>333103</if_matched_sid>
	<description>Free space monitor of a volume: backup deleted</description>
</rule>
