<rule id="300110" level="1">
	<if_group>sysmon_event7</if_group>
	<field name="win.eventdata.imageLoaded">ntdll.dll|sechost.dll</field>
	<description>Maybe a DLL for Reg</description>
</rule>
<rule id="300111" level="1">
	<if_group>sysmon_event7</if_group>
	<field name="win.eventdata.imageLoaded">ws2_32.dll|kernel32.dll</field>
	<description>Maybe a DLL for Reg</description>
</rule>
<rule id="300112" level="1">
	<if_group>sysmon_event7</if_group>
	<field name="win.eventdata.imageLoaded">msvcrt.dll|advapi32.dll</field>
	<description>Maybe a DLL for Reg</description>
</rule>
<rule id="300113" level="1">
	<if_group>sysmon_event7</if_group>
	<field name="win.eventdata.imageLoaded">rpcrt4.dll|kernelbase.dll</field>
	<description>Maybe a DLL for Reg</description>
</rule>
<rule id="300114" level="3" timeframe="5" frequency="2" >
	<same_field>win.system.computer</same_field>
	<if_matched_sid>300110</if_matched_sid>
	<if_matched_sid>300111</if_matched_sid>
	<if_matched_sid>300112</if_matched_sid>
	<if_matched_sid>300113</if_matched_sid>
	<description>Reg dump of credentials: DLLs with EventID 7</description>
</rule>
